<rules>

<!-- **************** N **************** -->

  <!-- année >> anâ, anâye, sêson -->
  <rule weight="0.8">
    <match lemma="année" tags="n.*"><select lemma="anâ"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="année" tags="n.*"><select lemma="sêson"/></match>
    <match lemma="scolaire" tags="adj.*"/>
  </rule>

  <!-- complexe >> complèxo, ensemblo -->
  <rule weight="0.8">
    <match lemma="complexe" tags="n.*"><select lemma="complèxo"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="complexe" tags="n.*"><select lemma="ensemblo"/></match>
    <or>
      <match lemma="sportif" tags="adj.*"/>
      <match lemma="industriel" tags="adj.*"/>
      <match lemma="hospitalier" tags="adj.*"/>
      <match lemma="commercial" tags="adj.*"/>
      <match lemma="récréatif" tags="adj.*"/>
      <match lemma="touristique" tags="adj.*"/>
    </or>
  </rule>

  <!-- conseil >> consèly, avis -->
  <rule weight="0.8">
    <match lemma="conseil" tags="n.*"><select lemma="consèly"/></match>
  </rule>

  <!-- dé >> diél, diâl, dê -->
  <rule weight="0.8">
