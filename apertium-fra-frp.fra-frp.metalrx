    <match tags="n.*"/>
  </rule>
  <rule weight="1.0">
    <match tags="det.*"/>
    <match lemma="propre" tags="adj.*"><select lemma="prôpro"/></match>
  </rule>

<!-- **************** V **************** -->

  <!-- transporter >> charrotar, transportar -->
  <rule weight="0.8">
    <match lemma="transporter" tags="vblex.*"><select lemma="charrotar"/></match>
  </rule>

  <!-- rattacher >> apondre, ratachiér -->
  <rule weight="0.8">
    <match lemma="rattacher" tags="vblex.*"><select lemma="apondre"/></match>
  </rule>

  <!-- disjoindre >> dèpondre, dèjuendre -->
  <rule weight="0.8">
    <match lemma="disjoindre" tags="vblex.*"><select lemma="dèpondre"/></match>
  </rule>

  <!-- revenir >> recampar, revenir -->
  <rule weight="0.8">
    <match lemma="revenir" tags="vblex.*"><select lemma="recampar"/></match>
  </rule>

  <!-- parvenir >> avengiér, aventar, avendre, parvenir -->
  <rule weight="0.8">
    <match lemma="parvenir" tags="vblex.*"><select lemma="avengiér"/></match>
  </rule>

  <!-- retourner >> reveriér, etc. -->
  <rule weight="0.8">
    <match lemma="retourner" tags="vblex.*"><select lemma="reveriér"/></match>
  </rule>

  <!-- regarder >> agouétiér, etc. -->
  <rule weight="0.8">
    <match lemma="regarder" tags="vblex.*"><select lemma="agouétiér"/></match>
  </rule>

  <!-- devenir >> venir, devegnir, devenir -->
  <rule weight="0.8">
    <match lemma="venir" tags="vblex.*"><select lemma="venir"/></match>
  </rule>

  <!-- jouer >> jouyér, menar -->
  <rule weight="0.8">
    <match lemma="jouer" tags="vblex.*"><select lemma="jouyér"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="jouer" tags="vblex.*"><select lemma="menar"/></match>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match lemma="de" tags="pr"/>
  </rule>

