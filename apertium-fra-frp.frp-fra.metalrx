      <match lemma="gâto" tags="n.*"/>
      <match lemma="gâtio" tags="n.*"/>
      <match lemma="quegnou" tags="n.*"/>
      <match lemma="quegnoua" tags="n.*"/>
      <match lemma="fllan" tags="n.*"/>
      <match lemma="crèssent" tags="n.*"/>
      <match lemma="radice" tags="n.*"/>
      <match lemma="megiér" tags="vblex.*"/>
      <match lemma="mengiér" tags="vblex.*"/>
      <match lemma="apondre" tags="vblex.*"/>
      <match lemma="rapondre" tags="vblex.*"/>
      <match lemma="ajoutar" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="6">
      <match tags="*"/>
    </repeat>
    <or>
      <match lemma="fllor" tags="n.*"><select lemma="crème"/></match>
      <match lemma="fllorèta" tags="n.*"><select lemma="crème"/></match>
    </or>
  </rule>

  <!-- homo, home >> homme, mari -->
  <rule weight="0.8">
    <or>
      <match lemma="homo" tags="n.*"><select lemma="homme"/></match>
      <match lemma="home" tags="n.*"><select lemma="homme"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <match tags="det.pos.*"/>
    <or>
      <match lemma="homo" tags="n.*"><select lemma="mari"/></match>
      <match lemma="home" tags="n.*"><select lemma="mari"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="homo" tags="n.*"><select lemma="mari"/></match>
      <match lemma="home" tags="n.*"><select lemma="mari"/></match>
    </or>
    <match lemma="de" tags="pr"/>
    <or>
      <match tags="np.ant.*"/>
      <match tags="np.cog.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="homo" tags="n.*"><select lemma="mari"/></match>
      <match lemma="home" tags="n.*"><select lemma="mari"/></match>
    </or>
    <match lemma="de" tags="pr"/>
    <match tags="det.*"/>
    <or>
      <seq n="personne"/>
      <seq n="parent"/>
    </or>
  </rule>

  <!-- papa >> papa, pape -->
  <rule weight="0.8">
    <match lemma="papa" tags="n.*"><select lemma="papa"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="papa" tags="n.*"><select lemma="pape"/></match>
    <match tags="np.ant.*"/>
  </rule>
  <rule weight="1.0">
    <match tags="det.def.*"/>
    <match lemma="papa" tags="n.*"><select lemma="pape"/></match>
  </rule>

  <!-- par >> paire, pair -->
  <!-- pâr >> paire, pair -->
  <!-- pàr >> paire, pair -->
  <rule weight="0.8">
    <or>
      <match lemma="par" tags="n.*"><select lemma="paire"/></match>
      <match lemma="pâr" tags="n.*"><select lemma="paire"/></match>
      <match lemma="pàr" tags="n.*"><select lemma="paire"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="par" tags="n.*"><select lemma="pair"/></match>
      <match lemma="pâr" tags="n.*"><select lemma="pair"/></match>
      <match lemma="pàr" tags="n.*"><select lemma="pair"/></match>
    </or>
    <match lemma="de" tags="pr"/>
    <match tags="np.top"/>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="par" tags="n.*"><select lemma="pair"/></match>
      <match lemma="pâr" tags="n.*"><select lemma="pair"/></match>
      <match lemma="pàr" tags="n.*"><select lemma="pair"/></match>
    </or>
    <match lemma="de" tags="pr"/>
    <match tags="det.*"/>
    <or>
      <match lemma="rê" tags="n*"/>
      <match lemma="royômo" tags="n*"/>
    </or>
  </rule>

  <!-- politico n.f. >> politique n.f., politique n.mf., politicien -->
  <rule weight="0.8">
    <match lemma="politico" tags="n.f.*"><select lemma="politique" tags="n.f.*"/></match>
  </rule>

  <!-- premiér >> premier, aîné -->
  <rule weight="0.8">
    <match lemma="premiér" tags="n.*"><select lemma="premier"/></match>
  </rule>

  <!-- règent >> instituteur, régent -->
  <rule weight="0.8">
    <match lemma="règent" tags="n.*"><select lemma="instituteur"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="règent" tags="n.*"><select lemma="régent"/></match>
    <match lemma="de" tags="pr"/>
    <repeat from="0" upto="1">
      <match tags="det.*"/>
    </repeat>
    <match tags="np.top"/>
  </rule>
  <rule weight="1.0">
    <match lemma="prince" tags="n.*"/>
    <match lemma="règent" tags="n.*"><select lemma="régent"/></match>
  </rule>

  <!-- sêson >> saison, année -->
  <rule weight="0.8">
    <match lemma="sêson" tags="n.*"><select lemma="saison"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="sêson" tags="n.*"><select lemma="année"/></match>
    <match lemma="scolèro" tags="adj.*"/>
  </rule>

  <!-- volontèro >> volontaire, bénévole -->
  <rule weight="0.8">
    <match lemma="volontèro" tags="n.*"><select lemma="volontaire"/></match>
  </rule>

<!-- **************** V **************** -->

  <!-- abosar >> démolir, écrouler -->
  <macro name="verb_nopron_pron" p1="abosar" p2="démolir" p3="écrouler" />

  <!-- apondre >> ajouter, rattacher -->
  <rule weight="0.8">
    <match lemma="apondre" tags="vblex.*"><select lemma="ajouter"/></match>
  </rule>

  <!-- aprèstar >> apprêter, préparer -->
  <macro name="verb_nopron_pron" p1="aprèstar" p2="apprêter" p3="préparer" />

  <!-- borrar >> bourrer, frapper, cogner -->
  <rule weight="0.8">
    <match lemma="borrar" tags="vblex.*"><select lemma="bourrer"/></match>
  </rule>

  <!-- bouciér >> bûcher, frapper, cogner -->
  <rule weight="0.8">
    <match lemma="bouciér" tags="vblex.*"><select lemma="bûcher"/></match>
  </rule>

  <!-- cachiér >> cacher, écraser -->
  <rule weight="0.8">
    <match lemma="cachiér" tags="vblex.*"><select lemma="cacher"/></match>
  </rule>

  <!-- enstalar >> installer, établir -->
  <rule weight="0.8">
    <match lemma="enstalar" tags="vblex.*"><select lemma="installer"/></match>
  </rule>

  <!-- enveyér >> envier, envoyer -->
  <rule weight="0.8">
    <match lemma="enveyér" tags="vblex.*"><select lemma="envier"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="enveyér" tags="vblex.*"><select lemma="envoyer"/></match>
    <repeat from="0" upto="6">
      <match tags="*"/>
    </repeat>
    <or>
      <match lemma="lètra" tags="n.*"/>
      <match lemma="dèpèche" tags="n.*"/>
    </or>
  </rule>

  <!-- molyér >> mouiller, impliquer -->
  <rule weight="0.8">
    <match lemma="molyér" tags="vblex.*"><select lemma="mouiller"/></match>
  </rule>

  <!-- rèvelyér, èvelyér, dèvelyér >> réveiller, éveiller  -->
  <macro name="verb_nopron_pron" p1="rèvelyér" p2="éveiller" p3="réveiller" />
  <macro name="verb_nopron_pron" p1="èvelyér" p2="éveiller" p3="réveiller" />
  <macro name="verb_nopron_pron" p1="dèvelyér" p2="éveiller" p3="réveiller" />
  <rule weight="1.2">
    <or>
      <match lemma="rèvelyér" tags="vblex.*"><select lemma="réveiller"/></match>
      <match lemma="èvelyér" tags="vblex.*"><select lemma="réveiller"/></match>
      <match lemma="dèvelyér" tags="vblex.*"><select lemma="réveiller"/></match>
    </or>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match tags="det.*"/>
    <or>
      <seq n="personne"/>
      <seq n="parent"/>
    </or>
  </rule>

  <!-- venir, vegnir >> venir, devenir -->
  <rule weight="0.8">
    <or>
      <match lemma="venir" tags="vblex.*"><select lemma="venir"/></match>
      <match lemma="vegnir" tags="vblex.*"><select lemma="venir"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="venir" tags="vblex.*"><select lemma="devenir"/></match>
      <match lemma="vegnir" tags="vblex.*"><select lemma="devenir"/></match>
    </or>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match tags="adj.*"/>
  </rule>

