<rules>

<!-- **************** PR **************** -->

  <!-- devant >> devant, avant -->
  <rule weight="0.8">
    <match lemma="devant" tags="pr.*"><select lemma="devant"/></match>
  </rule>

  <!-- aprés >> après, en train de -->
  <rule weight="0.8">
    <match lemma="aprés" tags="pr.*"><select lemma="après"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="vbser.*"/>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match lemma="aprés" tags="pr.*"><select lemma="en train de"/></match>
    <or>
      <match tags="vbser.inf"/>
      <match tags="vbhaver.inf"/>
      <match tags="vblex.inf"/>
      <match tags="vbmod.inf"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match tags="vbser.*"/>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match lemma="aprés" tags="pr.*"><select lemma="en train de"/></match>
    <match tags="prn.pro.*"/>
    <or>
      <match tags="vbser.inf"/>
      <match tags="vbhaver.inf"/>
      <match tags="vblex.inf"/>
      <match tags="vbmod.inf"/>
    </or>
  </rule>

<!-- **************** PRN **************** -->

  <!-- què >> que, quoi -->
  <rule weight="0.8">
    <match lemma="què" tags="prn.itg.*"><select lemma="que"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="què" tags="prn.itg.*"><select lemma="quoi"/></match>
    <match tags="sent"/>
  </rule>

  <!-- cél, quél, iquél >> celui-là, celui -->
  <rule weight="0.8">
    <or>
      <match lemma="cél" tags="prn.*"><select lemma="celui-là"/></match>
      <match lemma="quél" tags="prn.*"><select lemma="celui-là"/></match>
      <match lemma="iquél" tags="prn.*"><select lemma="celui-là"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="cél" tags="prn.*"><select lemma="celui"/></match>
      <match lemma="quél" tags="prn.*"><select lemma="celui"/></match>
      <match lemma="iquél" tags="prn.*"><select lemma="celui"/></match>
    </or>
    <or> 
      <match tags="rel.*"/>
      <match tags="pr"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="cél" tags="prn.*"><select lemma="celui"/></match>
      <match lemma="quél" tags="prn.*"><select lemma="celui"/></match>
      <match lemma="iquél" tags="prn.*"><select lemma="celui"/></match>
    </or>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match tags="adj.*"/>
  </rule>

  <!-- ço, ce, cen, quen, iquen >> cela, ceci, ce -->
  <rule weight="0.8">
    <or>
      <match lemma="ço" tags="prn.*"><select lemma="cela"/></match>
      <match lemma="ce" tags="prn.*"><select lemma="cela"/></match>
      <match lemma="cen" tags="prn.*"><select lemma="cela"/></match>
      <match lemma="quen" tags="prn.*"><select lemma="cela"/></match>
      <match lemma="iquen" tags="prn.*"><select lemma="cela"/></match>
    </or>
  </rule>

<!-- **************** REL **************** -->

  <!-- que >> que, qui, dont -->
  <rule weight="0.8">
    <match lemma="que" tags="rel.*"><select lemma="que"/></match>
  </rule>

  <!-- qui >> qui, celui qui -->
  <rule weight="0.8">
    <match lemma="qui" tags="rel.*"><select lemma="qui"/></match>
  </rule>

<!-- **************** POSTADV **************** -->

  <!-- que, qué >> ci, là -->
  <rule weight="0.8">
    <or>
      <match lemma="que" tags="postadv"><select lemma="là"/></match>
      <match lemma="qué" tags="postadv"><select lemma="là"/></match>
    </or>
  </rule>

<!-- **************** ADV **************** -->

  <!-- lé-en >> là-dedans, là-bas -->
  <rule weight="0.8">
    <match lemma="lé-en" tags="adv"><select lemma="là-dedans"/></match>
  </rule>

<!-- **************** V **************** -->

  <!-- apondre >> ajouter, rattacher -->
  <rule weight="0.8">
    <match lemma="apondre" tags="vblex.*"><select lemma="ajouter"/></match>
  </rule>

  <!-- cachiér >> cacher, écraser -->
  <rule weight="0.8">
    <match lemma="cachiér" tags="vblex.*"><select lemma="cacher"/></match>
  </rule>

  <!-- venir, vegnir >> venir, devenir -->
  <rule weight="0.8">
    <or>
      <match lemma="venir" tags="vblex.*"><select lemma="venir"/></match>
      <match lemma="vegnir" tags="vblex.*"><select lemma="venir"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="venir" tags="vblex.*"><select lemma="devenir"/></match>
      <match lemma="vegnir" tags="vblex.*"><select lemma="devenir"/></match>
    </or>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match tags="adj.*"/>
  </rule>

<!-- **************** NP **************** -->

  <macro name="noms_propres" p1="Jian" p2="Jean" />

